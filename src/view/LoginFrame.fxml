<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="140.0" prefWidth="222.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginFrameController">
   <center>
      <Pane prefHeight="222.0" prefWidth="253.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="title" alignment="CENTER" layoutX="14.0" layoutY="7.0" prefHeight="31.0" prefWidth="188.0" text="Title" textOverrun="CLIP">
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="43.0" text="Name" />
            <Label layoutX="14.0" layoutY="73.0" text="Password" />
            <TextField fx:id="name" layoutX="72.0" layoutY="39.0" prefHeight="25.0" prefWidth="141.0" />
            <TextField fx:id="password" layoutX="73.0" layoutY="69.0" prefHeight="25.0" prefWidth="140.0" />
            <Button fx:id="newUser" layoutX="13.0" layoutY="104.0" mnemonicParsing="false" onAction="#handleNewUserButtonClick" text="New" />
            <Button fx:id="connect" layoutX="148.0" layoutY="104.0" mnemonicParsing="false" onAction="#handleConnectButtonClick" text="Connect" />
         </children>
      </Pane>
   </center>
</BorderPane>
